@model mvcCourseProject.Models.ViewModels.UserViewModel

@{
    ViewBag.Title = "تسجيل الدخول";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>تسجيل الدخول</h3>
@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(u => u.UserName, new {@class="col-md-2"})
        @Html.TextBoxFor(u => u.UserName)
    </div>
    <div class="form-group">
        @Html.LabelFor(u => u.PassWord, new { @class = "col-md-2" })
        @Html.PasswordFor(u => u.PassWord)
    </div>
    <div class="form-group">
        @Html.CheckBoxFor(u => u.Remember)
        @Html.LabelFor(u => u.Remember)
    </div>
    <input type="submit" value="تسجيل" class="btn btn-success" />
}

